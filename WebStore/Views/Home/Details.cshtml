@model IEnumerable<WebStore.Models.Employee>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Летали</title>
</head>
<body>
    <table class="table table-bordered">
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.SurName</td>
                <td>@employee.Name</td>
                <td>@employee.MiddleName</td>
                <td>@employee.Age</td>
            </tr>
        }
    </table>

</body>
</html>
