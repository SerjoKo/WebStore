
@model IEnumerable<WebStore.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
    <h3>Сотрудники магазина</h3>
    <hr/>

    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.SurName</td>
                    <td>@employee.Name</td>
                    <td>@employee.MiddleName</td>
                    <td>@employee.Age</td>
                </tr>
            }
        </tbody>
    </table>

    <p>
        <a href="/">На главную</a>
    </p>
</body>
</html>
